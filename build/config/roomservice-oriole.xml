<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="lineage"
        fetch="https://github.com/LineageOS/"
        revision="lineage-22.1" />

    <remote name="themuppets"
        fetch="https://github.com/TheMuppets/"
        revision="lineage-22.1" />

    <!-- Device Tree -->
    <project name="android_device_google_oriole"
        path="device/google/oriole"
        remote="lineage" />

    <!-- Vendor Files -->
    <project name="proprietary_vendor_google_oriole"
        path="vendor/google/oriole"
        remote="themuppets" />
</manifest>